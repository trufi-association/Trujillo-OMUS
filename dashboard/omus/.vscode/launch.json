{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flutter Web (Puerto 3000)",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "3000"
      ]
    },
    {
      "name": "Flutter Web Debug",
      "type": "dart",
      "request": "launch",
      "program": "lib/main.dart",
      "args": [
        "-d",
        "chrome",
        "--web-port",
        "3000",
        "--web-browser-flag",
        "--auto-open-devtools-for-tabs"
      ]
    }
  ]
}
